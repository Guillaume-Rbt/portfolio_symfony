{% extends 'base.html.twig' %}


{% block title %}Guillaume Robert | Portfolio
{% endblock %}

{% block body %}

	<div class="accueil" style="background-image:url('{{ asset("assets/accueil.jpg") }}')">
	<div id="accueil-text" class="accueil-text">
		<h1>Guillaume Robert</h1>
		<p>Web designer / Développeur Front-end</p>
		</div>
		</div>
		
